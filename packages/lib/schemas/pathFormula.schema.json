{
  "$id": "PathFormula",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Value formula",
  "description": "Definition of a value formula",
  "type": "object",
  "properties": {
    "type": {
      "const": "path"
    },
    "path": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["type", "path"],
  "additionalProperties": true
}
