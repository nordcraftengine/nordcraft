{
  "$schema": "../../schemas/libFormula.schema.json",
  "name": "Round",
  "description": "Round a Number to the nearest decimal point.",
  "arguments": [
    {
      "name": "Input",
      "description": "Number to round.",
      "type": {
        "type": "Number"
      },
      "formula": {
        "type": "value",
        "value": 1
      }
    },
    {
      "name": "Decimals",
      "description": "Number of decimals to round to.",
      "type": {
        "type": "Number"
      },
      "formula": {
        "type": "value",
        "value": 0
      }
    }
  ],
  "output": {
    "type": {
      "type": "Number"
    },
    "description": "The rounded Number."
  },
  "mdnUrl": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Math/round"
}
